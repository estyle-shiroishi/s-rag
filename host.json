{
  "version": "2.0",
  "functionTimeout": "00:50:00",
  "extensions": {
    "queues": {
      "batchSize": 1,
      "newBatchThreshold": 0,
      "visibilityTimeout": "00:10:00",
      "maxDequeueCount": 5,
      "maxPollingInterval": "00:01:00"
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "singleton": {
    "lockPeriod": "00:15:00",
    "listenerLockPeriod": "00:15:00",
    "listenerLockRecoveryPollingInterval": "00:01:00",
    "lockAcquisitionTimeout": "00:15:00",
    "lockAcquisitionPollingInterval": "00:00:15"
  }
}